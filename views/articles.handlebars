<div class="container">
    {{#if articles}}
    {{#each articles}}
    <div class="card">
        <div class="card-header">
            <h3>
                <a href="{{this.link}}" class='article-link' target='_blank'
                    rel='noopener noreferrer'>{{this.title}}</a>
            </h3>
        </div>
    </div>
</div>